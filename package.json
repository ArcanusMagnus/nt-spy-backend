{
  "name": "hattrick_nt_analyzer",
  "version": "1.0.0",
  "description": "Web application made to analyze national teams in Hattrick.org, the legendary football manager game",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon dist/app.js",
    "build": "tsc",
    "dev": "ts-node-dev --cache-directory ./.ts-node-dev-cache --transpile-only --watch .env,.env.override --ignore-watch node_modules --poll src/app.ts",
    "debug": "ts-node-dev --transpile-only --watch .env,.env.override,test/.env --ignore-watch node_modules --inspect=0.0.0.0:9229 --poll --respawn src/app.ts"
  },
  "author": "Jan 'Arcanus' Majerníček",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.1",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "mongoose": "^6.1.8",
    "multer": "^1.4.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.13",
    "@types/nodemon": "^1.19.1",
    "@types/uuid": "^8.3.4",
    "nodemon": "^2.0.15",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
}
